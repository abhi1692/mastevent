<h1>Listing user_event_infos</h1>

<table>
  <thead>
    <tr>
      <th>Address</th>
      <th>Event exprience</th>
      <th>Preferred location</th>
      <th>Suitable day</th>
      <th>Suitable time</th>
      <th>Bike</th>
      <th>Qualification</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @user_event_infos.each do |user_event_info| %>
      <tr>
        <td><%= user_event_info.address %></td>
        <td><%= user_event_info.event_exprience %></td>
        <td><%= user_event_info.preferred_location %></td>
        <td><%= user_event_info.suitable_day %></td>
        <td><%= user_event_info.suitable_time %></td>
        <td><%= user_event_info.bike %></td>
        <td><%= user_event_info.qualification %></td>
        <td><%= link_to 'Show', user_event_info %></td>
        <td><%= link_to 'Edit', edit_user_event_info_path(user_event_info) %></td>
        <td><%= link_to 'Destroy', user_event_info, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New User event info', new_user_event_info_path %>
