<html>
	<head>
	<h2>Available Events </h2>
	</head>
	<body>
		<table border="0" cellpadding="0" cellspacing="0">

			<% if @events.present? %>

<%@events.each do |event|%><br>
<tr><%=event.event_name%></tr>

<tr><%= @not_success%></tr>



<%end%>
<%else%>

<%=@not_success%>

<%end%>
	

	
	</table>
	</body>
</html>
	